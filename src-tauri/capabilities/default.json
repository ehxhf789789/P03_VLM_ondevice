{
  "$schema": "https://raw.githubusercontent.com/nicegui-dev/nicegui/dev/tauri/tauri-config-v2-schema/capabilities-schema.json",
  "identifier": "default",
  "description": "Default capability set for the VLM Product Inspector app",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-open",
    "fs:default",
    "fs:allow-read",
    "fs:allow-write",
    "fs:allow-exists",
    "fs:allow-mkdir",
    "core:event:default",
    "core:event:allow-emit",
    "core:event:allow-listen"
  ]
}
